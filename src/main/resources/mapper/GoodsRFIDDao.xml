<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ServerLocal.dao.IGoodsRFIDDao">
    <select id="selectGoodsrfid" resultType="com.ServerLocal.model.Graduation_goods_rfid">
        select * from Graduation_goods_rfid
    </select>
    <delete id="delGoodsrfid" parameterType="java.lang.String">
        delete from Graduation_goods_rfid where RFID = #{RFID}
    </delete>
</mapper>
